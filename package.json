    "@octokit/rest": "^21.0.2",
    "dotenv": "^16.4.5",
    "inquirer": "^12.1.0",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/eslint__js": "^8.42.3",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3",
