{
  "name": "bitbucket-github-migrator",
  "version": "1.6.0",
  "description": "A tool to migrate repositories from Bitbucket to GitHub",
  "bin": {
    "bitbucket-github-migrator": "dist/index.js"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
